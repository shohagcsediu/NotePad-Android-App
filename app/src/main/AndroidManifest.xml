<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shohagapp.notepad" >

    <application

        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.shohagapp.notepad.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.shohagapp.notepad.Input_Note_DataActivity" android:parentActivityName="com.shohagapp.notepad.MainActivity" >
        </activity>
        <activity android:name="com.shohagapp.notepad.NoteViewActivity" android:parentActivityName="com.shohagapp.notepad.MainActivity">
        </activity>
    </application>

</manifest>